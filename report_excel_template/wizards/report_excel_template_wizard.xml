<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_report_excel_template_wizard" model="ir.ui.view">
            <field name="name">Report Excel</field>
            <field name="model">report.excel.template.wizard</field>
            <field name="arch" type="xml">
                <form string="Report Excel">
                    <group name="placeholder" col="4">
                    </group>
                    <footer>
                        <button name="export_excel" string="Export" type="object" class="btn-primary"/>
                        <button type="button" string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>